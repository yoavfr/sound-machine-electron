{
  "_comment": "Invoke using 'npm run-script package'. To create an installer use https://github.com/electron-userland/electron-builder",
  "name": "sound_machine",
  "version": "0.1.0",
  "main": "./main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ SoundMachine --all --out %userprofile%/Desktop/SoundMachine --version 0.30.2 --overwrite --icon=./app/img/app-icon.icns"
  },
  "devDependencies": {
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.30.3"
  },
  "dependencies": {
    "nconf": "^0.7.2"
  }
}
